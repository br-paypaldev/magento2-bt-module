<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="frame-src">
            <values>
                <value id="braintrepayments_com" type="host">*.braintreepayments.com</value>
                <value id="assets_braintreegateway_com" type="host">assets.braintreegateway.com</value>
                <value id="paypal_com" type="host">*.paypal.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="braintree_api_com" type="host">*.braintree-api.com</value>
            </values>
        </policy>
        <policy id="script-src">
            <values>
                <value id="paypal_com" type="host">*.paypal.com</value>
            </values>
        </policy>
        <policy id="img-src">
            <values>
                <value id="paypal_com" type="host">*.paypal.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
